<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_vertical"
    android:background="@drawable/rentibeijing"
    >
    <ImageView
        android:id="@+id/allshenti"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:background="@drawable/tou_houqian" />

    <ImageView
        android:id="@+id/bozi1bu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/allshenti"
        android:layout_centerHorizontal="true"
        android:background="@drawable/jingbu_houqian" />

    <ImageView
        android:id="@+id/xiong1bu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/yaobu1bu"
        android:layout_below="@+id/jian1bu"
        android:background="@drawable/bei_houqian" />

    <ImageView
        android:id="@+id/shoubi2bu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/jian1bu"
        android:layout_toStartOf="@+id/xiong1bu"
        android:background="@drawable/shoubi2_houqian" />

    <ImageView
        android:id="@+id/jian1bu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bozi1bu"
        android:layout_toEndOf="@+id/shoubi2bu"
        android:background="@drawable/jian_houqian" />

    <ImageView
        android:id="@+id/shoubi1bu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/jian1bu"
        android:layout_toEndOf="@+id/xiong1bu"
        android:background="@drawable/shoubi1_houqian" />

    <ImageView
        android:id="@+id/yaobu1bu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/xiong1bu"
        android:layout_centerHorizontal="true"
        android:background="@drawable/yao_houqian" />


    <ImageView
        android:id="@+id/shengzhiqi1bu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/yaobu1bu"
        android:layout_centerHorizontal="true"
        android:background="@drawable/tun_houqian" />

    <ImageView
        android:id="@+id/tuibu1bu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/shengzhiqi1bu"
        android:layout_below="@+id/shengzhiqi1bu"
        android:background="@drawable/tui_houqian" />

    <ImageView
        android:id="@+id/shouzuo1bu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/shengzhiqi1bu"
        android:layout_marginEnd="21dp"
        android:layout_marginTop="42dp"
        android:layout_toStartOf="@+id/shengzhiqi1bu"
        android:background="@drawable/shou2_houqian" />

    <ImageView
        android:id="@+id/shouyou1bu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/shouzuo1bu"
        android:layout_alignEnd="@+id/shoubi1bu"
        android:background="@drawable/shou1_houqian" />









    <ImageView
        android:id="@+id/head"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:background="@drawable/tou_houhoua" />

    <ImageView
        android:id="@+id/jingbu_hou"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/allshenti"
        android:layout_centerHorizontal="true"
        android:background="@drawable/jingbu_houhoua" />

    <ImageView
        android:id="@+id/xiongbu_hou"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/yaobu1bu"
        android:layout_below="@+id/jian1bu"
        android:background="@drawable/bei_houhoua" />

    <ImageView
        android:id="@+id/zuoshoubi_hou"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/jian1bu"
        android:layout_toStartOf="@+id/xiong1bu"
        android:background="@drawable/shoubi2_houhoua" />

    <ImageView
        android:id="@+id/jian_hou"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bozi1bu"
        android:layout_toEndOf="@+id/shoubi2bu"
        android:background="@drawable/jian_houhoua" />

    <ImageView
        android:id="@+id/youshoubi_hou"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/jian1bu"
        android:layout_toEndOf="@+id/xiong1bu"
        android:background="@drawable/shoubi1_houhoua" />

    <ImageView
        android:id="@+id/yaobu_hou"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/xiong1bu"
        android:layout_centerHorizontal="true"
        android:background="@drawable/yao_houhoua" />


    <ImageView
        android:id="@+id/shengzhiqi_hou"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/yaobu_hou"
        android:layout_toEndOf="@+id/zuoshou_hou"
        android:background="@drawable/tun_houhoua" />

    <ImageView
        android:id="@+id/tuibu_hou"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/shengzhiqi_hou"
        android:layout_toEndOf="@+id/zuoshou_hou"
        android:background="@drawable/tui_houhoua" />

    <ImageView
        android:id="@+id/zuoshou_hou"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/shouyou1bu"
        android:layout_marginEnd="21dp"
        android:layout_toStartOf="@+id/shengzhiqi1bu"
        android:background="@drawable/shou2_houhoua" />

    <ImageView
        android:id="@+id/youshou_hou"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/shouyou1bu"
        android:layout_alignTop="@+id/shouyou1bu"
        android:background="@drawable/shou1_houhoua" />

</RelativeLayout>