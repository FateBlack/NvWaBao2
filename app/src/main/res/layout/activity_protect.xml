            android:iconifiedByDefault="false"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginTop="110dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:queryHint="按功效查询"
            android:id="@+id/searchView">
        </SearchView>-->

    </RelativeLayout>
    <ImageView
        android:id="@+id/searchView"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignEnd="@+id/textView3"
        android:layout_below="@+id/textView2"

        android:background="@drawable/searchviewpng">

    </ImageView>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="2">

        <TextView

            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:gravity="center_horizontal"
            android:text="食物分类"
            android:textColor="@color/viewfinder_mask" />

        <ImageView
            android:id="@+id/guwu"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_alignStart="@+id/yaocai"
            android:layout_alignTop="@+id/dounairu"
            android:background="@drawable/guwu" />

        <ImageView
            android:id="@+id/dounairu"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_alignStart="@+id/shuiguoganguo"
            android:layout_below="@+id/shucai"
            android:layout_marginTop="13dp"
            android:background="@drawable/dounairu" />

        <ImageView
            android:id="@+id/gandou"
            android:layout_width="60dp"
            android:layout_height="60dp"

            android:layout_alignStart="@+id/shuichan"
            android:layout_alignTop="@+id/qinshou"
            android:background="@drawable/gandou" />

        <ImageView
            android:id="@+id/qinshou"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_alignStart="@+id/guwu"
            android:layout_alignTop="@+id/shucai"
            android:background="@drawable/qinshou" />

        <ImageView
            android:id="@+id/shucai"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_alignStart="@+id/dounairu"
            android:layout_below="@+id/textView"
            android:layout_marginTop="17dp"
            android:background="@drawable/shucai" />

        <ImageView
            android:id="@+id/shuichan"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_alignTop="@+id/guwu"
            android:layout_marginStart="41dp"
            android:layout_toEndOf="@+id/guwu"
            android:background="@drawable/shuichan" />

        <ImageView
            android:id="@+id/shuiguoganguo"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/dounairu"
            android:layout_marginStart="39dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/shuiguoganguo" />

        <ImageView
            android:id="@+id/yaocai"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_alignTop="@+id/shuiguoganguo"
            android:layout_centerHorizontal="true"
            android:background="@drawable/yaocai" />
        <ImageView
            android:id="@+id/tiaoweipin"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_alignStart="@+id/shuichan"
            android:layout_alignTop="@+id/yaocai"
            android:background="@drawable/tiaoweipin" />
        <ImageView
            android:id="@+id/huahuilei"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_alignStart="@+id/shuiguoganguo"
            android:layout_below="@+id/shuiguoganguo"
            android:layout_marginTop="12dp"
            android:background="@drawable/huahuilei" />
        <ImageView
            android:id="@+id/chalei"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_alignStart="@+id/yaocai"
            android:layout_alignTop="@+id/huahuilei"
            android:background="@drawable/chalei" />





    </RelativeLayout>
</LinearLayout>
